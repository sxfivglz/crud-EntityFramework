@model PracticaUno.Models.Inventories

@{
    ViewData["Title"] = "Details";
}
<br />
<h2 class="text-center">Inventory details</h2>

<div>

    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Warehouse
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Warehouses.warehouse_name)
        </dd>
        <dt class = "col-sm-2">
            Quantity
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.quantity)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.product_id" class="btn btn-outline-primary">Edit</a>
    <a asp-action="Index" class="btn btn-outline-secondary">Back to List</a>
</div>
