@model PracticaUno.Models.Warehouses

@{
    ViewData["Title"] = "Details";
}
<br />
<h2 class="text-center">Warehouse details</h2>
<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Warehouse
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.warehouse_name)
        </dd>
        <dt class = "col-sm-2">
           Location
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Locations.address)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.warehouse_id" class="btn btn-outline-primary">Edit</a>
    <a asp-action="Index" class="btn btn-outline-secondary">Back to List</a>
</div>
